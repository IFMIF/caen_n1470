record(ao, "$(SS):$(DD)_$(CC):VSet") {
  field(DESC, "Commande V")
  field(DTYP, "Soft Channel")
  field(OMSL, "supervisory")
  field(FLNK,"$(SS):$(DD)_$(CC):VCalc")
  field(PREC, "2")
  field(EGU, "kV")
  field(DRVH, "$(V_DRVH)")
  field(DRVL, "0.0")
  field(HOPR, "$(V_HOPR)")
  field(LOPR, "0.0")
}

record(calcout, "$(SS):$(DD)_$(CC):VCalc") {
  field(DESC, "kV en V")
  field(DTYP, "stream")
  field(INPA, "1000.0")
  field(INPB, "$(SS):$(DD)_$(CC):VSet.VAL")
  field(CALC, "A*B")
  field(OOPT, "Every Time")
#  field(DOPT, "CALC")
  field(OUT, "@psCaen_proto.db setV($(board),$(channel)) N1470")
}

